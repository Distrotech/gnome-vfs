# setup the environment

mkdir /tmp/test
cp test.input /tmp/test/test.input
cd /tmp/test

# simple tests on the file filing system

cp test.input t
mv t t2
cp t2 t
open #1 t
read #1 16
seek #1 0
read #1 16
open #2 t2
read #2 16
close #1
close #2
rm t
rm t2

