<?xml version="1.0"?>
<gconfschemafile>
  <schemalist>

    <schema>   
      <key>/schemas/desktop/gnome/url-handlers/trash/enabled</key>
      <applyto>/desktop/gnome/url-handlers/trash/enabled</applyto>
      <owner>gnome</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>How to handle trash URLs</short>
        <long>Set to true to have a program specified in trash/command handle trash URLs</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/gnome/url-handlers/trash/command</key>
      <applyto>/desktop/gnome/url-handlers/trash/command</applyto>
      <owner>gnome</owner>
      <type>string</type>
      <default>nautilus "%s"</default>
      <locale name="C">
        <short>URL handler for trash pages</short>
        <long>URL handler for trash pages</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/gnome/url-handlers/trash/need-terminal</key>
      <applyto>/desktop/gnome/url-handlers/trash/need-terminal</applyto>
      <owner>gnome</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Run program in terminal</short>
        <long>True if the program to handle this URL should be run in a terminal</long>
      </locale>
    </schema>
    
  </schemalist>
</gconfschemafile>
